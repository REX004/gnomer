<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".session2.NewPassword"
    android:background="@color/white">

    <Button
        android:id="@+id/button3"
        android:layout_width="342dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="510dp"
        android:background="@drawable/button_sign"
        android:text="Log in"
        android:fontFamily="@font/roboto_bold"
        app:backgroundTint="@color/grey2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/nameET10"
        android:layout_width="342dp"
        android:layout_height="44dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edit_text_background"
        android:ems="10"
        android:hint="**********"
        android:inputType="textPassword"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email5" />

    <EditText
        android:id="@+id/nameET7"
        android:layout_width="342dp"
        android:layout_height="44dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edit_text_background"
        android:ems="10"
        android:hint="**********"
        android:inputType="textPassword"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email3" />

    <TextView
        android:id="@+id/email5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Confirm Password"
        android:textColor="@color/grey2"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/nameET10"
        app:layout_constraintTop_toBottomOf="@+id/nameET7" />

    <TextView
        android:id="@+id/email3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="279dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Password"
        android:textColor="@color/grey2"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/nameET7"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/welcome_back3"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="155dp"
        android:fontFamily="@font/roboto_medium"
        android:text="New Password"
        android:textColor="@color/grey"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/complete4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/roboto_medium"
        android:text="Enter new password"
        android:textColor="@color/grey2"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/welcome_back3"
        app:layout_constraintTop_toBottomOf="@+id/welcome_back3" />

</androidx.constraintlayout.widget.ConstraintLayout>